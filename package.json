{
  "name": "ez-loc",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil App Starter",
  "scripts": {
    "build": "stencil build",
    "format": "prettier './**/*.{ts,tsx,js,cjs,css,scss,html,mdx,json}' './*.{ts,cjs,md,json}' --write --cache",
    "generate": "stencil generate",
    "lint": "eslint src --ext ts,tsx --fix --cache",
    "start": "stencil build --dev --watch --serve",
    "watch": "stencil build --watch"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "3.7.2",
    "@stencil-community/router": "^1.0.2",
    "@stencil/core": "latest",
    "@stencil/sass": "^3.0.4",
    "@tailwindcss/forms": "^0.5.0",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "eslint": "^8.42.0",
    "eslint-define-config": "1.17.0",
    "eslint-plugin-no-relative-import-paths": "1.5.2",
    "prettier": "^2.8.8",
    "prettier-define-config": "1.0.1",
    "prettier-plugin-package": "1.3.0",
    "rollup-plugin-dotenv": "^0.5.0",
    "stencil-tailwind-plugin": "^1.8.0",
    "tailwindcss": "3.0.23",
    "typescript": "^5.1.3"
  },
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.0.0",
    "@stencil-community/eslint-plugin": "^0.5.0",
    "@supabase/supabase-js": "^2.24.0",
    "flowbite": "^1.6.5",
    "notiflix": "^3.2.6"
  }
}
